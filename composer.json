{
	"name": "mathysie/foulard",
	"description": "FooBar Ondersteunende Utility t.b.v. Laven, Alcohol en Reuze Drankjes",
	"keywords": [
		"foulard",
		"FooBar"
	],
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Mathijs Kolkhuis Tanke",
			"email": "mathijs@kolkhuistanke.nl"
		}
	],
	"autoload": {
		"psr-4": {
			"app\\": "app"
		}
	},
	"require": {
		"php": ">=7.0.0",
		"mako/framework": "5.5.*",
		"bertptrs/marty": "^5.2",
		"twbs/bootstrap": "^4.1"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"scripts": {
		"post-update-cmd": [
			"ln -fs ../../../vendor/twbs/bootstrap/dist/css/bootstrap.min.css public/assets/css/bootstrap.min.css",
			"",
			"ln -fs ../../../vendor/twbs/bootstrap/dist/js/bootstrap.min.js public/assets/js/bootstrap.min.js"
		]
	},
	"require-dev": {
		"phpro/grumphp": "^0.14.1",
		"friendsofphp/php-cs-fixer": "^2.12",
		"jakub-onderka/php-parallel-lint": "^1.0",
		"phpstan/phpstan": "^0.9.2",
		"roave/security-advisories": "dev-master",
		"sebastian/phpcpd": "^3.0"
	}
}
